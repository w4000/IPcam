NVT_TURNKEY_CDDCNN_AI2_PATH=$(NVT_HDAL_DIR)/vendor/third_party/cddcnn_ai2
NVT_TURNKEY_PDCNN_AI2_PATH=$(NVT_HDAL_DIR)/vendor/third_party/pdcnn_ai2
NVT_TURNKEY_FDCNN_AI2_PATH=$(NVT_HDAL_DIR)/vendor/third_party/fdcnn_ai2
NVT_TURNKEY_PVDCNN_AI2_PATH=$(NVT_HDAL_DIR)/vendor/third_party/pvdcnn
NVT_TURNKEY_CDDCNN_AI2_PATH=$(NVT_HDAL_DIR)/vendor/third_party/cddcnn_ai2
NVT_TURNKEY_PDCNN_FASTBOOT_PATH=$(NVT_HDAL_DIR)/vendor/third_party/pdcnn_fastboot
NVT_TURNKEY_LIMIT_FDET_PATH=$(NVT_HDAL_DIR)/vendor/third_party/limit_fdet
NVT_TURNKEY_MD_PATH=$(NVT_HDAL_DIR)/vendor/third_party/motion_detection

all:
	@$(MAKE) -C $(NVT_TURNKEY_CDDCNN_AI2_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_FDCNN_AI2_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_PVDCNN_AI2_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_PDCNN_AI2_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_CDDCNN_AI2_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_PDCNN_FASTBOOT_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_LIMIT_FDET_PATH)
	@$(MAKE) -C $(NVT_TURNKEY_MD_PATH)
install:
	@$(MAKE) -C $(NVT_TURNKEY_CDDCNN_AI2_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_FDCNN_AI2_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_PVDCNN_AI2_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_PDCNN_AI2_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_CDDCNN_AI2_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_PDCNN_FASTBOOT_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_LIMIT_FDET_PATH) install
	@$(MAKE) -C $(NVT_TURNKEY_MD_PATH) install
clean:	
	@$(MAKE) -C $(NVT_TURNKEY_CDDCNN_AI2_PATH) clean
	@$(MAKE) -C $(NVT_TURNKEY_FDCNN_AI2_PATH) clean
	@$(MAKE) -C $(NVT_TURNKEY_PVDCNN_AI2_PATH) clean		
	@$(MAKE) -C $(NVT_TURNKEY_PDCNN_AI2_PATH) clean	
	@$(MAKE) -C $(NVT_TURNKEY_CDDCNN_AI2_PATH) clean	
	@$(MAKE) -C $(NVT_TURNKEY_PDCNN_FASTBOOT_PATH) clean
	@$(MAKE) -C $(NVT_TURNKEY_LIMIT_FDET_PATH) clean
	@$(MAKE) -C $(NVT_TURNKEY_MD_PATH) clean
