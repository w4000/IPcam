proc		/proc			proc	defaults	0	0
sysfs		/sys			sysfs	defaults	0	0
tmpfs		/dev			tmpfs	defaults	0	0
tmpfs       /tmp            tmpfs   defaults,noatime,nosuid,nodev,exec,mode=1777 0 0
tmpfs		/var/run		tmpfs	defaults,rw,nosuid,mode=0755	0	0
tmpfs		/var/lock		tmpfs	defaults,rw,nosuid,mode=0755	0	0
tmpfs		/var/log		tmpfs	defaults,rw,nosuid,mode=0755	0	0
debugfs		/sys/kernel/debug	debugfs	defaults	0	0
